{
    "manifest_version": "2",
    "required_api_version": "3",
    "name": "Ulauncher lolcate extension",
    "developer_name": "xorly, compilelife, ncatanchin",
    "icon": "images/icon.png",
    "instructions": "This extensions need <code>lolcate</code>",
    "query_debounce": 0.5,
    "preferences": [
      {
        "id": "keyword",
        "type": "keyword",
        "name": "Ulauncher lolcate extension",
        "description": "Search files using lolcate",
        "default_value": "s"
      },
      {
        "id": "limit",
        "type": "input",
        "name": "maximum number of result",
        "description": "result will be truncate to the maximum number",
        "default_value": "16"
      }
    ]
  }
